<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<?import java.net.URL?>
<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="cz.cvut.fel.zahorto2.animalworld.Controler.MainMenu">
    <stylesheets>
        <URL value="@/css/mainMenu.css"/>
    </stylesheets>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Label text="Animal World!"/>
    <Button text="New World" onAction="#newButtonEvent"/>
    <Button text="Load World" onAction="#loadButtonEvent"/>
    <Button text="Quit" onAction="#quitButtonEvent"/>
</VBox>
